{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://scrumdaddy-and-the-devs.netlify.com/",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --open --mode development",
    "lint": "./node_modules/.bin/eslint src",
    "flow": "flow --quiet",
    "test": "yarn lint && yarn flow && yarn build",
    "deploy": "yarn build && netlify deploy --prod --dir=dist",
    "deploy:staging": "yarn build && netlify deploy --dir=dist",
    "generate": "yarn graphql-codegen"
  },
  "husky": {
    "hooks": {
      "pre-push": "if git-branch-is master; then yarn lint && yarn flow; else echo \"\nSkipping checks because you're on a dev branch\nManually check with \\033[1myarn test\\033[0m!\n\"; fi"
    }
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-upload-client": "^11.0.0",
    "faker": "^4.1.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "lodash.debounce": "^4.0.8",
    "markdown-to-jsx": "^7.0.1",
    "react": "^16.10.1",
    "react-day-picker": "^7.4.0",
    "react-dom": "^16.10.1",
    "react-router-dom": "^5.1.2",
    "semantic-ui-react": "^0.88.1",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@graphql-codegen/cli": "^1.8.2",
    "@graphql-codegen/fragment-matcher": "^1.8.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-graphql-tag": "^2.5.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^6.1.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.109.0",
    "git-branch-is": "^3.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.8",
    "netlify-cli": "^2.19.4",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}
